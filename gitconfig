[alias]
    pr = pull --rebase
    s = status -sb
    c = commit
    cm = commit -m
    cma = commit -a -m
    br = branch
    co = checkout
    d = diff
    ds = diff --stat
    l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
    lg = log -p
    ll = log --pretty=format:'%h %ad | %s%d [%an]' --decorate --graph --date=short
    lla = log --pretty=format:'%h %ad | %s%d [%an]' --decorate --graph --date=short --all
    ls = ls-files
[core]
    autocrlf = input
    editor = "emacs -nw --no-init-file"
[branch]
